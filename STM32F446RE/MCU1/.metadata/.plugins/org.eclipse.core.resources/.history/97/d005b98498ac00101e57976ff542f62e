#include "stm32f446re.h"

void delay(void)
{
    for (uint32_t i = 0; i < 1000000; i++);
}

int main(void)
{
    RCC->AHB1ENR |= (1 << 0);      // Enable GPIOA clock
    GPIOA->MODER |= (1 << 10);     // Set PA5 as output (MODER5 = 01)

    while (1)
    {
        GPIOA->ODR ^= (1 << 5);    // Toggle PA5
        delay();
    }
}

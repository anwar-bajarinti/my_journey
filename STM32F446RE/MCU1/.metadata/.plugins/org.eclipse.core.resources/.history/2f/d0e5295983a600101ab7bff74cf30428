#include <stdint.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif
#define   RCC_CR   (0x40023800UL)
#define   RCC_CFGR  (0x40023800UL+ 0x08UL)

int main(void)
{
    /* Loop forever */
	uint32_t  *prccCR=(unint32_t*)RCC_CR;
	uint32_t  *prccCFGR=(uint32*)RCC_CFGR;
	 *prccCR=1<<18;
	 while( !()*prccCR&(1<<17)));
	 *prccCR=1<<16;
	 *prccCFGR=1<<22;

	for(;;);
}
